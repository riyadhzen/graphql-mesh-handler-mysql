{
  "name": "@graphql-mesh/mysql",
  "version": "0.17.22",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/handlers/mysql"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "scripts": {
    "build": "yarn transpile-ts && bob build",
    "transpile-ts": "tsc --project tsconfig.build.json"
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "0.2.6",
    "@graphql-mesh/store": "0.8.55",
    "@graphql-mesh/string-interpolation": "0.3.2",
    "@graphql-mesh/types": "0.85.0",
    "@graphql-mesh/utils": "0.41.22",
    "@graphql-tools/delegate": "9.0.8",
    "@graphql-tools/utils": "8.12.0",
    "graphql-compose": "9.0.9",
    "graphql-fields": "2.0.3",
    "graphql-scalars": "1.19.0",
    "mysql": "2.18.1",
    "mysql-utilities": "1.1.4",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@types/graphql-fields": "1.3.4",
    "@types/mysql": "2.15.21",
    "bob-the-bundler": "^4.0.0",
    "typescript": "4.8.4"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
